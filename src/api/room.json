{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Represents a game room.",
  "properties": {
    "gameID": {
      "type": "string"
    },
    "players": {
      "items": {
        "properties": {
          "id": {
            "enum": [0, 1],
            "type": "number"
          },
          "name": {
            "type": "string"
          }
        },
        "required": ["id"],
        "type": "object"
      },
      "type": "array"
    },
    "setupData": {}
  },
  "required": ["gameID", "players"],
  "type": "object"
}
